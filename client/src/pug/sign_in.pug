div.jumbotron
	div.container
		div.row
			div.col-sm-6.col-md-4.col-md-offset-4
				div.form-group
					label Email:
					input.form-control(type="text", [(ngModel)]="model.email")
				div.form-group
					label Password:
					input.form-control(type="password")
				button.btn.btn-success(type="submit", (click)="signin()") Login
